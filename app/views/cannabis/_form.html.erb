<%= form_for @cannabis do |f| %>
    <%= f.text_field :strain, placeholder: "strain" %>
    <br>
    <%= f.text_field :data, placeholder: "description" %>
    <br>
    <%= f.submit "Save changes" %>
    <br>
    <%= button_to "Delete #{@cannabis.strain}", cannabi_path(@cannabis), method: :delete %>

<% end %>